<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MessageBoard">
    <ListView fx:id="messageListView" prefHeight="200.0" />
    <TextField fx:id="messageTextField" promptText="Type your message here" />
    <Button text="Post Message" onAction="#postMessage" />
</VBox>

